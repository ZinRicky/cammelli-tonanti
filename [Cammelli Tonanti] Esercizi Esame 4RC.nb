Cell[BoxData[
 RowBox[{
  RowBox[{"confrontoColore", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"xmin_", ",", "xmax_", ",", "ymin_", ",", "ymax_"}], "}"}], ",", 
    "ndiv_", ",", "nit_"}], "]"}], ":=", 
  RowBox[{"DynamicModule", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"z0", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1."}], ",", "0."}], "}"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"j", "=", 
       RowBox[{"Compile", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"z", ",", "_Complex"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"n", ",", "_Integer"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"c", ",", "_Complex"}], "}"}]}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Module", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"k", "=", "0"}], ",", 
             RowBox[{"w", "=", "z"}], ",", 
             RowBox[{"r", "=", 
              RowBox[{".5", "+", 
               RowBox[{"Sqrt", "[", 
                RowBox[{".25", "+", 
                 RowBox[{"Abs", "[", "c", "]"}]}], "]"}]}]}]}], "}"}], ",", 
           RowBox[{
            RowBox[{"While", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"Abs", "[", "w", "]"}], "<", "r"}], "&&", 
               RowBox[{"k", "<", "nit"}]}], ",", 
              RowBox[{
               RowBox[{"w", "=", 
                RowBox[{
                 SuperscriptBox["w", "2"], "+", "c"}]}], ";", 
               RowBox[{"k", "++"}]}]}], "]"}], ";", 
            RowBox[{"0.7", " ", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"k", "+", "1"}], ")"}], "/", 
              RowBox[{"(", 
               RowBox[{"1", "+", "n"}], ")"}]}]}]}]}], "]"}]}], 
        "\[IndentingNewLine]", "]"}]}], ",", "\[IndentingNewLine]", "jgrid", 
      ",", "jset", ",", "mgrid", ",", "mset"}], "\[IndentingNewLine]", "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"jgrid", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"xmin2_", ",", "xmax2_", ",", "ymin2_", ",", "ymax2_"}], 
         "}"}], ",", "ndiv2_", ",", "nit2_", ",", "c_"}], "]"}], ":=", 
      RowBox[{"ParallelTable", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"j", "[", 
         RowBox[{
          RowBox[{"x", "+", 
           RowBox[{"I", " ", "y"}]}], ",", "nit2", ",", "c"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"y", ",", "ymin2", ",", "ymax2", ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"ymax2", "-", "ymin2"}], ")"}], "/", "ndiv2"}]}], "}"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "xmin2", ",", "xmax2", ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"xmax2", "-", "xmin2"}], ")"}], "/", "ndiv2"}]}], "}"}]}],
        "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"jset", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"xmin2_", ",", "xmax2_", ",", "ymin2_", ",", "ymax2_"}], 
         "}"}], ",", "ndiv2_", ",", "numit_", ",", "c_"}], "]"}], ":=", 
      RowBox[{"Graphics", "@", 
       RowBox[{"Raster", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"jgrid", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"xmin2", ",", "xmax2", ",", "ymin2", ",", "ymax2"}], 
            "}"}], ",", "ndiv2", ",", "numit", ",", "c"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"xmin2", ",", "ymin2"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"xmax2", ",", "ymax2"}], "}"}]}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"ColorFunction", "\[Rule]", 
          RowBox[{"Function", "[", 
           RowBox[{"a", ",", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"a", "\[Equal]", "0.7"}], ",", "Black", ",", 
              RowBox[{
               RowBox[{"ColorData", "[", 
                RowBox[{"{", 
                 RowBox[{"\"\<SunsetColors\>\"", ",", "\"\<Reverse\>\""}], 
                 "}"}], "]"}], "[", "a", "]"}]}], "]"}]}], "]"}]}]}], 
        "\[IndentingNewLine]", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"mgrid", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"xmin2_", ",", "xmax2_", ",", "ymin2_", ",", "ymax2_"}], 
         "}"}], ",", "ndiv2_", ",", "nit2_"}], "]"}], ":=", 
      RowBox[{"ParallelTable", "[", 
       RowBox[{
        RowBox[{"j", "[", 
         RowBox[{"0", ",", "nit2", ",", 
          RowBox[{"cx", "+", 
           RowBox[{"I", " ", "cy"}]}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"cy", ",", "ymin2", ",", "ymax2", ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"ymax2", "-", "ymin2"}], ")"}], "/", "ndiv2"}]}], "}"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"cx", ",", "xmin2", ",", "xmax2", ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"xmax2", "-", "xmin2"}], ")"}], "/", "ndiv2"}]}], "}"}]}],
        "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"mset", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"xmin2_", ",", "xmax2_", ",", "ymin2_", ",", "ymax2_"}], 
         "}"}], ",", "ndiv2_", ",", "nit2_"}], "]"}], ":=", 
      RowBox[{"Graphics", "@", 
       RowBox[{"Raster", "[", 
        RowBox[{
         RowBox[{"mgrid", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"xmin2", ",", "xmax2", ",", "ymin2", ",", "ymax2"}], 
            "}"}], ",", "ndiv2", ",", "nit2"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"xmin2", ",", "ymin2"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"xmax2", ",", "ymax2"}], "}"}]}], "}"}], ",", 
         RowBox[{"ColorFunction", "\[Rule]", 
          RowBox[{"Function", "[", 
           RowBox[{"a", ",", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"a", "\[Equal]", "0.7"}], ",", "Black", ",", 
              RowBox[{
               RowBox[{"ColorData", "[", 
                RowBox[{"{", 
                 RowBox[{"\"\<SunsetColors\>\"", ",", "\"\<Reverse\>\""}], 
                 "}"}], "]"}], "[", "a", "]"}]}], "]"}]}], "]"}]}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"GraphicsRow", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Show", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"mset", "[", 
            RowBox[{
             RowBox[{"N", "@", 
              RowBox[{"{", 
               RowBox[{"xmin", ",", "xmax", ",", "ymin", ",", "ymax"}], 
               "}"}]}], ",", "ndiv", ",", "nit"}], "]"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"Graphics", "[", 
            RowBox[{"Locator", "[", 
             RowBox[{
              RowBox[{"Dynamic", "@", "z0"}], ",", 
              RowBox[{"Graphics", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"Red", ",", 
                  RowBox[{"Point", "[", 
                   RowBox[{"{", 
                    RowBox[{"0", ",", "0"}], "}"}], "]"}], ",", 
                  RowBox[{"Circle", "[", "]"}]}], "}"}], ",", 
                RowBox[{"ImageSize", "\[Rule]", "12"}]}], "]"}]}], "]"}], 
            "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"Frame", "\[Rule]", "True"}], ",", 
           RowBox[{"ImageSize", "\[Rule]", 
            RowBox[{"Scaled", "[", "0.4", "]"}]}], ",", 
           RowBox[{"PlotLabel", "\[Rule]", 
            RowBox[{"Dynamic", "@", 
             RowBox[{"TraditionalForm", "[", 
              RowBox[{"Chop", "[", 
               RowBox[{"Complex", "@@", "z0"}], "]"}], "]"}]}]}]}], 
          "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
         RowBox[{"Dynamic", "@", 
          RowBox[{"Show", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"jset", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"xmin", ",", "xmax", ",", "ymin", ",", "ymax"}], "}"}],
               ",", "ndiv", ",", "nit", ",", 
              RowBox[{"Complex", "@@", "z0"}]}], "]"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"ListPlot", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"Re", "[", "#", "]"}], ",", 
                  RowBox[{"Im", "[", "#", "]"}]}], "}"}], "&"}], "/@", 
               RowBox[{"Take", "[", 
                RowBox[{
                 RowBox[{"NestList", "[", 
                  RowBox[{
                   RowBox[{
                    RowBox[{
                    RowBox[{"Complex", "@@", "z0"}], "+", 
                    SuperscriptBox["#", "2"]}], "&"}], ",", "0", ",", "50"}], 
                  "]"}], ",", 
                 RowBox[{"-", "10"}]}], "]"}]}], ",", 
              RowBox[{"PlotRange", "\[Rule]", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{"xmin", ",", "xmax"}], "}"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"ymin", ",", "ymax"}], "}"}]}], "}"}]}], ",", 
              RowBox[{"PlotStyle", "\[Rule]", "Red"}]}], "]"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"Frame", "\[Rule]", "True"}], ",", 
            RowBox[{"ImageSize", "\[Rule]", 
             RowBox[{"Scaled", "[", "0.4", "]"}]}]}], "\[IndentingNewLine]", 
           "]"}]}]}], "\[IndentingNewLine]", "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ImageSize", "\[Rule]", "Full"}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Program",

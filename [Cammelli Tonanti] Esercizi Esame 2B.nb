Cell[BoxData[
 RowBox[{
  RowBox[{"hash", "[", "str_", "]"}], ":=", 
  RowBox[{"BaseForm", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Fold", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"Mod", "[", 
        RowBox[{
         RowBox[{"#1", " ", 
          RowBox[{"Times", "@@", "#2"}]}], ",", 
         SuperscriptBox["2", "256"]}], "]"}], "&"}], ",", 
      "\[IndentingNewLine]", "1", ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"2", 
        RowBox[{"(", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"#", "/.", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j__"}], "}"}], "\[Rule]", 
              RowBox[{"{", 
               RowBox[{
                SuperscriptBox["i", "16"], ",", "j"}], "}"}]}], "}"}]}], 
           "&"}], "/@", "\[IndentingNewLine]", 
          RowBox[{"Partition", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"PadRight", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"ToCharacterCode", "[", "str", "]"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"StringLength", "[", "str", "]"}], "+", 
               RowBox[{"Mod", "[", 
                RowBox[{
                 RowBox[{"32", "-", 
                  RowBox[{"StringLength", "[", "str", "]"}]}], ",", 
                 "\[IndentingNewLine]", "32"}], "]"}]}], ",", 
              "\[IndentingNewLine]", "32"}], "]"}], ",", 
            "\[IndentingNewLine]", "32"}], "]"}]}], 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"Questa", " ", "ricorrenza", " ", "del", " ", "32"}], ",", 
           " ", 
           RowBox[{"bench\[EAcute]", " ", "curiosa"}], ",", " ", 
           RowBox[{"non", " ", "\[EGrave]", " ", 
            RowBox[{"voluta", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
         ")"}]}], "+", "1"}]}], "\[IndentingNewLine]", "]"}], ",", 

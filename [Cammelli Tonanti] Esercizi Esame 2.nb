       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"p0", " ", "q0"}], ",", "a", ",", 
         RowBox[{"keyb", "[", 
          RowBox[{"p0", ",", "q0", ",", "a"}], "]"}]}], "}"}]}]}], 
     "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"mab", "[", "k", "]"}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",

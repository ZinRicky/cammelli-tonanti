Cell[TextData[{
 "La crittazione alla Hill \[EGrave] stata fatta usando una matrice intera ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Times]", "2"}], TraditionalForm]],ExpressionUUID->
  "45820089-ceb8-4c06-b1f2-eead8fe68bd2"],

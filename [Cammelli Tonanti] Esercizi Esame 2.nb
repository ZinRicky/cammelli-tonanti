Cell[BoxData[{
 RowBox[{
  RowBox[{"mab", "[", "k_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "p", ",", "q", ",", "m", ",", "a", ",", "b", ",", "primecheck", ",", 
      "coprimecheck"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"primecheck", "[", 
        RowBox[{"{", 
         RowBox[{"p1_", ",", "q1_"}], "}"}], "]"}], ":=", 
       RowBox[{"If", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"And", "[", 
          RowBox[{
           RowBox[{"p1", "\[NotEqual]", "q1"}], ",", 
           RowBox[{"PrimeQ", "[", "p1", "]"}], ",", 
           RowBox[{"PrimeQ", "[", "q1", "]"}]}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"p1", ",", "q1"}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{"Prime", "@", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              SuperscriptBox["10", "k"], ",", 
              SuperscriptBox["10", 
               RowBox[{"k", "+", "2"}]]}], "}"}], ",", "2"}], "]"}]}]}], 
        "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"p", ",", "q"}], "}"}], "=", 
       RowBox[{"FixedPoint", "[", 
        RowBox[{"primecheck", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"coprimecheck", "[", 
         RowBox[{"p2_", ",", "q2_"}], "]"}], "[", "n_", "]"}], ":=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"CoprimeQ", "[", 
          RowBox[{"n", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"p2", "-", "1"}], ")"}], 
            RowBox[{"(", 
             RowBox[{"q2", "-", "1"}], ")"}]}]}], "]"}], ",", "n", ",", 
         RowBox[{"Prime", "@", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{
             SuperscriptBox["10", 
              RowBox[{"Floor", "[", 
               FractionBox["k", "2"], "]"}]], ",", 
             SuperscriptBox["10", 
              RowBox[{
               RowBox[{"Floor", "[", 
                FractionBox["k", "2"], "]"}], "+", "2"}]]}], "}"}], "]"}]}]}],
         "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"a", "=", 
       RowBox[{"FixedPoint", "[", 
        RowBox[{
         RowBox[{"coprimecheck", "[", 
          RowBox[{"p", ",", "q"}], "]"}], ",", "0"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"m", "=", 
       RowBox[{"p", " ", "q"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"b", "=", 
       RowBox[{"keyb", "[", 
        RowBox[{"p", ",", "q", ",", "a"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"ClearAll", "[", 
       RowBox[{"p", ",", "q"}], "]"}], ";", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"In", " ", "questo", " ", "modo"}], ",", " ", 
        RowBox[{
        "si", " ", "elimina", " ", "ogni", " ", "possibilit\[AGrave]", " ", 
         "di", " ", "conoscere", " ", "i", " ", "primi", " ", "p"}], ",", " ", 
        RowBox[{"q", " ", "generati", " ", "casualmente"}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"m", ",", "a", ",", "b"}], "}"}]}], "\[IndentingNewLine]", 
     ")"}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mab", "[", 
   RowBox[{"p0_", ",", "q0_", ",", "k_"}], "]"}], ":=", 
  RowBox[{"If", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"And", "[", 
     RowBox[{
      RowBox[{"p0", "\[NotEqual]", "q0"}], ",", 
      RowBox[{"PrimeQ", "[", "p0", "]"}], ",", 
      RowBox[{"PrimeQ", "[", "q0", "]"}]}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"coprimecheck", ",", "a"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"coprimecheck", "[", 
          RowBox[{"p2_", ",", "q2_"}], "]"}], "[", "n_", "]"}], ":=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"CoprimeQ", "[", 
           RowBox[{"n", ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"p2", "-", "1"}], ")"}], 
             RowBox[{"(", 
              RowBox[{"q2", "-", "1"}], ")"}]}]}], "]"}], ",", "n", ",", 
          RowBox[{"Prime", "@", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{
              SuperscriptBox["10", 
               RowBox[{"Floor", "[", 
                FractionBox["k", "2"], "]"}]], ",", 
              SuperscriptBox["10", 
               RowBox[{
                RowBox[{"Floor", "[", 
                 FractionBox["k", "2"], "]"}], "+", "2"}]]}], "}"}], 
            "]"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 

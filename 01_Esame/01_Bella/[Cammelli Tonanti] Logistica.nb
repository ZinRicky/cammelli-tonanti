  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", "1"}], "]"}]}], TraditionalForm]],ExpressionUUID->
  "d7bf1d8e-5b0b-4171-ba99-57c1cbdef826"],

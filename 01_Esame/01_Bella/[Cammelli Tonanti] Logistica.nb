     RowBox[{
      RowBox[{
       RowBox[{"RootReduce", "[", 
        RowBox[{"f", "[", 
         RowBox[{"\[Mu]", ",", "#"}], "]"}], "]"}], "&"}], ",", "x", ",", 
      "n"}], "]"}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"fnCList", "=", 
     RowBox[{"Compile", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[Mu]", ",", "_Real"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "_Real"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "_Integer"}], "}"}]}], "}"}], ",", 
       RowBox[{"NestList", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"\[Mu]", " ", "#", 
           RowBox[{"(", 
            RowBox[{"1", "-", "#"}], ")"}]}], "&"}], ",", "x", ",", "n"}], 
        "]"}]}], "]"}]}], ";"}]}]}]], "Input",

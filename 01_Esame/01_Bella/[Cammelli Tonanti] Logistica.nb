Cell[BoxData[
 RowBox[{
  RowBox[{"(*", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Definiamo", " ", 
     RowBox[{
      SubscriptBox["f", "\[Mu]"], ".", "\[IndentingNewLine]", "Definiamo"}], 
     " ", "subito", " ", "anche", " ", 

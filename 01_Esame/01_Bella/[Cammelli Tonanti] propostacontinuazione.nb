  FormBox[
   TemplateBox[{"f", "\[Mu]", 
     SuperscriptBox["2", "n"]},
    "Subsuperscript"], TraditionalForm]],
  CellChangeTimes->{{3.8038983036146636`*^9, 3.803898307369625*^9}},
  ExpressionUUID->"83c0ef8a-fe8d-4a62-a454-196d6b423bcc"],

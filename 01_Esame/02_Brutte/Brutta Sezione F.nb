  RowBox[{"\[Mu]", " ", "x", 
   RowBox[{"(", 
    RowBox[{"1", "-", "x"}], ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fn", "[", 
   RowBox[{"\[Mu]_", ",", "x_", ",", "n_"}], "]"}], ":=", " ", 
  RowBox[{"Nest", "[", 
   RowBox[{

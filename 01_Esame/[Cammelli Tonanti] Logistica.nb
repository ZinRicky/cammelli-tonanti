Cell[TextData[{
 "Con ",
 StyleBox["mappa logistica",
  FontSlant->"Italic"],
 " intendiamo la funzione polinomiale reale ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["f", "\[Mu]"], "(", "x", ")"}], "=", 
    RowBox[{"\[Mu]", " ", "x", " ", 

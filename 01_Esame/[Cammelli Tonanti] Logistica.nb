Cell[TextData[{
 "Con ",
 StyleBox["mappa logistica",
  FontSlant->"Italic"],
 " intendiamo la funzione polinomiale reale ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["f", "\[Mu]"], "(", "x", ")"}], "=", 
    RowBox[{"\[Mu]", " ", "x", " ", 
     RowBox[{"(", 
      RowBox[{"1", "-", "x"}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b82e42e5-6a5e-4e97-9d39-0d91c6b50d30"],

Cell[BoxData[
 RowBox[{
  RowBox[{"cmplxL", "[", "t_", "]"}], ":=", " ", 
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], 
    FractionBox[
     RowBox[{"1", "+", 
      SuperscriptBox[
       RowBox[{"Cos", "[", "1", "]"}], "2"]}], 
     RowBox[{
      RowBox[{"Cos", "[", "1", "]"}], 
      RowBox[{"Sin", "[", "1", "]"}]}]], 
    FractionBox[
     RowBox[{
      RowBox[{"Sin", "[", "t", "]"}], 
      RowBox[{"Cos", "[", "t", "]"}]}], 
     RowBox[{"1", "+", 
      SuperscriptBox[
       RowBox[{"Cos", "[", "t", "]"}], "2"]}]]}], "+", " ", 
   RowBox[{"\[ImaginaryI]", " ", 
    SqrtBox["2"], 
    FractionBox[
     RowBox[{"Sin", "[", "t", "]"}], 
     RowBox[{"1", "+", 
      SuperscriptBox[
       RowBox[{"Cos", "[", "t", "]"}], "2"]}]]}]}]}]], "Input",
 CellChangeTimes->{{3.795257080303232*^9, 3.795257118446411*^9}, {
  3.79562946040084*^9, 
  3.795629461144396*^9}},ExpressionUUID->"3dd8bdf6-68ad-4df3-9a56-\
54c6c5e6fc81"],


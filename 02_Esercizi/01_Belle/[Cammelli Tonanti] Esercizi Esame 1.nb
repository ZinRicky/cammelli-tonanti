Cell[TextData[{
 "ove ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalL]", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "348ebe97-6ab5-4453-8724-d8e0e3892379"],
 " \[EGrave] la lemniscata e ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a0973fda-671c-4554-aaff-f2641e07d96e"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bb96e5df-13a3-459e-87ad-1d9677ac13e2"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "3"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "97e6c854-f02f-4cae-a665-e88bfcd75e2a"],
 " sono le tre radici di ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "3"], "+", 
    RowBox[{
     RowBox[{"a", "(", "t", ")"}], " ", "x"}], "+", 
    RowBox[{"b", "(", "t", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cb3c44f7-4519-4e1c-972b-87e27be97718"],
 ", con ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "faea321c-ac78-4731-80e9-a75056edcb29"],
 " e ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7b6daea1-57c9-4fa2-a3e9-e24e8fa5ccd4"],
 " che variano sulla cardioide."
}], "Text",
 CellChangeTimes->{{3.795609191935898*^9, 
  3.795609366826932*^9}},ExpressionUUID->"b887bfe7-94b5-4295-b3cd-\
63ee80070284"],

Cell[TextData[{
 "Ci\[OGrave] \[EGrave] vero se e solo se, posto ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"z", "(", "\[Tau]", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["lemn", "x"], "(", "\[Tau]", ")"}], "+", 
     RowBox[{"\[ImaginaryI]", " ", 
      RowBox[{
       SubscriptBox["lemn", "y"], "(", "\[Tau]"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bd70fb73-c8a1-4982-9b43-691afcc288e7"],
 "), \[EGrave] vero che per ogni ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Tau]", "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", 
      RowBox[{"2", "\[Pi]"}]}], "]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "06ab5426-e185-4ae8-9819-4e95f8c662b9"],
 " esiste ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", 
      RowBox[{"2", "\[Pi]"}]}], "]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0f41b344-3a70-4c70-96ef-d045c5177547"],
 " tale che ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"z", "(", "\[Tau]", ")"}], "3"], "+", 
     RowBox[{
      RowBox[{"a", "(", "t", ")"}], 
      RowBox[{"z", "(", "\[Tau]", ")"}]}], "+", 
     RowBox[{"b", "(", "t", ")"}]}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "99c29384-d14d-4267-ab4c-d155bbd84766"],
 ". Definiamo una funzione che restituisca la norma del polinomio calcolato \
sulle coordinate della cardioide; ci\[OGrave] \[EGrave] necessario perch\
\[EAcute], quando calcoleremo il polinomio pi\[UGrave] avanti, restituir\
\[AGrave] principalmente valori complessi \[LongDash] e a noi interessa \
visualizzare i risultati come grafico di una funzione ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DoubleStruckCapitalR]", "\[Rule]", "\[DoubleStruckCapitalR]"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b2337703-fdcb-4aa1-bb5e-f49839f942fc"],
 "."
}], "Text",
 CellChangeTimes->{{3.7956093814354467`*^9, 3.795609394473423*^9}, {

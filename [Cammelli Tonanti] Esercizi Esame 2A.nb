Cell[BoxData[{
 RowBox[{
  RowBox[{"listRSA", "[", 
   RowBox[{"aprime_", ",", "mprime_", ",", "b_", ",", "m_"}], "]"}], ":=", 
  " ", 
  RowBox[{
   RowBox[{"PowerMod", "[", 
    RowBox[{
     RowBox[{"PowerMod", "[", 
      RowBox[{"#", ",", "aprime", ",", "mprime"}], "]"}], ",", "b", ",", 
     "m"}], "]"}], "&"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rsaCrypt1", "[", 
   RowBox[{"aprime_", ",", "mprime_", ",", "b_", ",", "m_", ",", "msg_"}], 
   "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"listRSA", "[", 
    RowBox[{"b", ",", "m", ",", "aprime", ",", "mprime"}], "]"}], "@", 
   RowBox[{"ToCharacterCode", "[", "msg", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rsaDecrypt1", "[", 
    RowBox[{"bprime_", ",", "mprime_", ",", "a_", ",", "m_", ",", "lst_"}], 
    "]"}], ":=", " ", 
   RowBox[{"FromCharacterCode", "[", 
    RowBox[{
     RowBox[{"listRSA", "[", 
      RowBox[{"a", ",", "m", ",", "bprime", ",", "mprime"}], "]"}], "@", 
     "lst"}], "]"}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rsaCrypt", "[", 
   RowBox[{"aprime_", ",", "mprime_", ",", "b_", ",", "m_", ",", "msg_"}], 
   "]"}], ":=", " ", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"mprime", ">", "m"}], ",", 
    RowBox[{"rsaCrypt1", "[", 
     RowBox[{"aprime", ",", "mprime", ",", "b", ",", "m", ",", "msg"}], "]"}],
     ",", 
    RowBox[{"rsaCrypt1", "[", 
     RowBox[{"b", ",", "m", ",", "aprime", ",", "mprime", ",", "msg"}], 
     "]"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rsaDecrypt", "[", 
   RowBox[{"bprime_", ",", "mprime_", ",", "a_", ",", "m_", ",", "lst_"}], 
   "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"mprime", ">", "m"}], ",", 
    RowBox[{"rsaDecrypt1", "[", 
     RowBox[{"bprime", ",", "mprime", ",", "a", ",", "m", ",", "lst"}], "]"}],
     ",", 
    RowBox[{"rsaDecrypt1", "[", 
     RowBox[{"a", ",", "m", ",", "bprime", ",", "mprime", ",", "lst"}], 
     "]"}]}], "]"}]}]}], "Input",

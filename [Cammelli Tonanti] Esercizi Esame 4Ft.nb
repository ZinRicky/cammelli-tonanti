     "\t\t\t\t\t\t\t\t", 
     RowBox[{"ranges2", "=", 
      RowBox[{"Append", "[", 
       RowBox[{
        RowBox[{"Drop", "[", 
         RowBox[{"ranges", ",", "1"}], "]"}], ",", 
        RowBox[{"Last", "@", "ranges"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t\t\t\t\t\t\t\t", 
     RowBox[{"rangesfull", "=", " ", 
      RowBox[{"Partition", "[", 
       RowBox[{
        RowBox[{"Riffle", "[", 
         RowBox[{"j", ",", "ranges2"}], "]"}], ",", "2"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t\t\t\t\t\t\t\t", 
     RowBox[{"jr", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Show", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"#", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", 
           RowBox[{"Graphics", "[", 
            RowBox[{"{", 
             RowBox[{"Thick", ",", "Red", ",", 
              RowBox[{"Line", "[", 
               RowBox[{"rectOutline", "[", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "]"}], "]"}]}], "}"}], 
            "]"}]}], "}"}], "]"}], "&"}], "/@", "rangesfull"}]}], ";", 
     "\[IndentingNewLine]", "\t\t\t\t\t\t\t", 
     RowBox[{"GraphicsRow", "[", 
      RowBox[{"jr", ",", 
       RowBox[{"ImageSize", "\[Rule]", "Full"}]}], "]"}]}]}], 

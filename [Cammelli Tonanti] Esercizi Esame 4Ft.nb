 RowBox[{
  RowBox[{"zoomJSet", "[", 
   RowBox[{"c_", ",", "ranges_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"j", ",", "ranges2", ",", "jr", ",", "rangesfull"}], "}"}], ",", 
    "\[IndentingNewLine]", "\t\t\t\t\t\t\t\t", 
    RowBox[{
     RowBox[{"j", "=", 

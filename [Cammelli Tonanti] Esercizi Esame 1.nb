Cell[BoxData[
 RowBox[{"{", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Limit", "[", 
    RowBox[{
     FractionBox[
      RowBox[{"PrimePi", "[", "n", "]"}], 
      RowBox[{"n", "/", 
       RowBox[{"Log", "[", "n", "]"}]}]], ",", 
     RowBox[{"n", "\[Rule]", "\[Infinity]"}]}], "]"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Limit", "[", 
    RowBox[{
     FractionBox[
      RowBox[{"PrimePi", "[", "n", "]"}], 
      RowBox[{"LogIntegral", "[", "n", "]"}]], ",", 

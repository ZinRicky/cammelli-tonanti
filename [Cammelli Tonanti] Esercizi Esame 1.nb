  FormBox[
   RowBox[{"[", 
    RowBox[{
     RowBox[{"-", "10"}], ",", "10"}], "]"}], TraditionalForm]],
  ExpressionUUID->"60804aea-73f8-48b1-8db8-a1dc8c728acd"],
